{
  "MONGO_HOSTNAME": {
    "type": "string",
    "default": "localhost",
    "required": true
  },
  "MONGO_DB_NAME": {
    "type": "string",
    "test": "fhir-test",
    "development": "fhir-development",
    "default": "fhir",
    "required": true
  },
  "WHITELIST": {
    "type": "string",
    "default": "http://localhost"
  },
  "LOGGING_LEVEL": {
    "type": "string",
    "default": "debug"
  },
  "SERVER_PORT": {
    "type": "number",
    "default": 8090
  },
  "RESOURCE_SERVER": {
    "type": "string",
    "default": "http://localhost:8090"
  },
  "AUTH_SERVER_URI": {
    "type": "string",
    "default": "http://localhost:8090"
  }
}

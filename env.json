{
  "MONGO_URL": {
    "type": "string",
    "default": "mongodb://127.0.0.1:27017",
    "required": true
  },
  "MONGO_DB_NAME": {
    "type": "string",
    "test": "fhir-test",
    "development": "fhir-development",
    "default": "remsadmin",
    "required": true
  },
  "WHITELIST": {
    "type": "string",
    "default": "http://localhost, http://localhost:3005"
  },
  "LOGGING_LEVEL": {
    "type": "string",
    "default": "debug"
  },
  "SERVER_PORT": {
    "type": "number",
    "default": 8090
  },
  "RESOURCE_SERVER": {
    "type": "string",
    "default": "http://localhost:8090"
  },
  "AUTH_SERVER_URI": {
    "type": "string",
    "default": "http://localhost:8090"
  },
  "VSAC_API_KEY": {
    "type": "string",
    "default": "changeMe"
  }
}
